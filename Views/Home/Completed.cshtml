@model Register
@{
    ViewData["Title"] = "Completed";
}

<div class="bg-primary text-white p-1 m-1">
    <h2>Register is Completed</h2>
</div>

<table class="table table-striped table-bordered">
    <tr>
        <th>UserName</th>
        <td>@Model.UserName</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Birth Date</th>
        <td>@Model.BirthDate.ToString("d")</td>
    </tr>
</table>